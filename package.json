{
  "name": "yygen",
  "version": "1.0.0-beta.2",
  "description": "Project template creation tools",
  "license": "MIT",
  "bin": {
    "yygen": "./bin/yygen"
  },
  "sideEffects": false,
  "scripts": {
    "clear": "rm -rf dist",
    "build": "npm run clear && mkdir dist && cp -R bin templates LICENSE package.json README.md .npmignore dist",
    "publish2npm": "npm run build && npm publish dist"
  },
  "dependencies": {
    "fs-extra": "^7.0.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/yargs": "^11.1.1"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanyuantingfeng/yygen.git"
  },
  "author": "nanyuantingfeng",
  "bugs": {
    "url": "https://github.com/nanyuantingfeng/yygen/issues"
  },
  "homepage": "https://github.com/nanyuantingfeng/yygen#readme"
}
